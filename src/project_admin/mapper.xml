<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.example.BlogMapper">


	<insert id="userIns" parameterType="project_admin.UserVO">
    insert into MEMBER(M_ID, M_PW, M_EMAIL, M_NAME, M_BIRTH, M_PHONE, M_TERMS, M_CLASS) 
    values(#{m_id},#{m_pw},#{m_email},#{m_name},#{m_birth},#{m_phone},#{m_terms},#{m_class})
    </insert>

	
	<select id="membersList" resultType="project_admin.UserVO">
	    select * from member order by m_id
	</select>
	
	<delete id="memberDel" parameterType="project_admin.UserVO">
		delete from member where m_id = #{m_id}
	</delete>
	
	<select id="memberId" parameterType="String" resultType="project_admin.UserVO">
		select * from member where m_id= #{m_id}
	</select>
	
	<select id="memberName" parameterType="String" resultType="project_admin.UserVO">
		select * from member where m_name= #{m_name}
	</select>
	
	<select id="memberEmail" parameterType="String" resultType="project_admin.UserVO">
		select * from member where m_email= #{m_email}
	</select>
	
	
	<select id="idChk" parameterType="String" resultType="project_admin.UserVO">
		select m_id from member where m_id= #{m_id}
	</select>
	
</mapper>









